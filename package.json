{
  "name": "vue-3dmodel-viewer",
  "version": "0.1.0",
  "description": "Three.js model viewer for Vue",
  "keywords": [
    "vue",
    "three.js",
    "component"
  ],
  "author": "Aleksandr Tar <XpycT.TOP@gmail.com>",
  "repository": "https://github.com/XpycT/vue-3dmodel-viewer",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name ModelViewer src/model-viewer.vue --dest lib",
    "build:wc": "vue-cli-service build --target wc --name model-viewer src/model-viewer.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "screenfull": "^3.3.2",
    "three": "^0.94.0",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "vue-template-compiler": "^2.5.16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
